{
  "title": "@regraf/session-local - Documentation",
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "lib/",
      "./README.md"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "encoding": "utf8",
    "template": "node_modules/docdash",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false
    }
  },
  "docdash": {
    "static": true,
    "sort": true,
    "sectionOrder": [
      "Modules",
      "Namespaces",
      "Classes",
      "Externals",
      "Events",
      "Mixins",
      "Tutorials",
      "Interfaces"
    ],
    "search": true,
    "collapse": false,
    "typedefs": true,
    "meta": {
      "title": "Regraf Session local Documentation",
      "keyword": "Telegram Regraf Session Local Database JSON XML",
      "description": "Documentation for Regraf Session local"
    },
    "menu": {
      "GitHub Repository ⧉": {
        "href": "https://github.com/regraf-bots/session-local",
        "target": "_blank",
        "class": "menu-item",
        "id": "link_repo"
      },
      "NPM Package ⧉": {
        "href": "https://www.npmjs.com/package/@regraf/session-local",
        "target": "_blank",
        "class": "menu-item",
        "id": "link_npm"
      }
    }
  }
}
